app: photoshop.exe
app: Adobe Photoshop
-
new (file | pane): key(ctrl-n)
open file: key(ctrl-o)
close file: key(ctrl-w)
transform: key(ctrl-t)
deselect: key(ctrl-d)
new layer: key(ctrl-shift-alt-n)
open folder: key(ctrl-shift-o)
save as: key(ctrl-shift-s)
step backwards [<user.n20>]: key("ctrl-alt-z:{n20 or 1}")
step forwards [<user.n20>]: key("ctrl-shift-z:{n20 or 1}")
brush size down [<user.n20>]: key("[:{n20 or 1}")
brush size up [<user.n20>]: key("]:{n20 or 1}")