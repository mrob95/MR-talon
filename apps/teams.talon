title: /Microsoft Teams/
-
tag(): user.command_mode
new chat: key(alt-n)
go to search: key(ctrl-e)
go to activity: key(ctrl-shift-1)
go to chat: key(ctrl-shift-2)
go to teams: key(ctrl-shift-3)
go to calls: key(ctrl-shift-4)
go to files: key(ctrl-shift-5)
go next <user.n20>: key("alt-up:{n20}")
go previous <user.n20>: key("alt-down:{n20}")
