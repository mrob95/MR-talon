app: /.*/
and title: /.*\.md$/
app: Notepad
app: notepad.exe
-
insert image:
    "![]()"
    key(left:3)
insert link:
    "[]()"
    key(left:3)
insert in-line code:
    "``"
    key(left)
insert code block:
    "``````"
    key(left:3 enter:2 up)
insert horizontal rule: "---"
insert heading: "# "
insert subheading: "## "
insert sub sub heading: "### "
insert reference:
    "[@]"
    key(left)
insert italics:
    "__"
    key(left)
insert bold text:
    "****"
    key(left:2)
